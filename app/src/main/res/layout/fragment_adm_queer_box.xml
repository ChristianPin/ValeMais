<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".adm.fragment.AdmQueerBoxFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="468dp"
        android:orientation="vertical">

        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewProdutosQueerBox"
            android:layout_width="303dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:layout_marginTop="32dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp">


            <ImageView
                android:layout_width="99dp"
                android:layout_height="120dp"
                android:layout_gravity="left|center_vertical"
                android:layout_marginStart="10dp"
                android:src="@drawable/ic_produto_dispo" />

            <TextView
                android:id="@+id/textViewProdutosQueerBox"
                android:layout_width="224dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginLeft="88dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Produtos Queer Box"
                android:textColor="@color/pretob"
                android:textSize="15sp" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewCadQueerBox"
            android:layout_width="303dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:layout_marginTop="32dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp">


            <ImageView
                android:layout_width="99dp"
                android:layout_height="120dp"
                android:layout_gravity="left|center_vertical"
                android:layout_marginStart="10dp"
                android:src="@drawable/ic_queer_box" />

            <TextView
                android:id="@+id/textViewCadQueerBox"
                android:layout_width="224dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginLeft="88dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Cadastrar Queer Box"
                android:textColor="@color/pretob"
                android:textSize="15sp" />

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardViewListaQueerBox"
            android:layout_width="303dp"
            android:layout_height="120dp"
            android:layout_gravity="center"
            android:layout_marginTop="32dp"
            app:cardCornerRadius="20dp"
            app:cardElevation="10dp">

            <ImageView
                android:layout_width="111dp"
                android:layout_height="120dp"
                android:layout_gravity="start|center_vertical"
                android:paddingLeft="10dp"
                android:src="@drawable/ic_lista_queer" />

            <TextView
                android:id="@+id/textListaQueerBox"
                android:layout_width="218dp"
                android:layout_height="20dp"
                android:layout_gravity="center"
                android:layout_marginLeft="88dp"
                android:fontFamily="@font/poppins_medium"
                android:text="Lista de Queer Box"
                android:textColor="@color/pretob"
                android:textSize="15sp" />


        </androidx.cardview.widget.CardView>

    </LinearLayout>

</FrameLayout>